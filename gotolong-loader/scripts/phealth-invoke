#!/usr/bin/env python

import sys

import phealth.phealth

import cutil.config_json

cmd_line = cutil.config_json.main(['config_json.py', 'phealth'])
print(cmd_line)

cmd_line_list = cmd_line.split(' ')
print(cmd_line_list)

cmd_line_list.insert(0, 'phealth.py')

# NOTE: Is it good to modify the sys.argv
sys.argv = cmd_line_list
phealth.phealth.main()
