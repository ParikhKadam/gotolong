#!/usr/bin/env python

import sys

import corpact.corpact

import cutil.config_json

sys.argv = ['config_json.py', 'corpact']
cmd_line = cutil.config_json.main()
print(cmd_line)

cmd_line_list = cmd_line.split(' ')
print(cmd_line_list)

cmd_line_list.insert(0, 'corpact.py')

# NOTE: Is it good to modify the sys.argv
sys.argv = cmd_line_list
corpact.corpact.main()
